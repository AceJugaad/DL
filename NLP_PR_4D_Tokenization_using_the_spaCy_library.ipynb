        "import spacy\n",
        "nlp = spacy.blank(\"en\")\n",
        "\n",
        "str = \"I love to study Natural Language Processing in Python\"\n",
        "doc = nlp(str)\n",
        "\n",
        "words = [word.text for word in doc]\n",
        "print(words)"
